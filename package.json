{
  "name": "git-visualizer",
  "displayName": "Git Visualizer",
  "publisher": "AnonymousAAArdvark",
  "description": "A tool to better visualize git branches meant to be used alongside Learn-Git",
  "icon": "images/logo.png",
  "version": "0.0.6",
  "engines": {
    "vscode": "^1.68.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AnonymousAAArdvark/Git-Visualizer"
  },
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "viewsContainers": {
      "activitybar": [
        {
          "id": "git-visualizer-view",
          "title": "Git Visualizer",
          "icon": "images/icon.svg"
        }
      ]
    },
    "views": {
      "git-visualizer-view": [
        {
          "type": "webview",
          "id": "git-visualizer",
          "name": "Git Visualizer",
          "icon": "images/icon.svg",
          "contextualTitle": "Git Visualizer"
        }
      ]
    },
    "commands": [
      {
        "command": "git-visualizer.visualize",
        "title": "Git Visualizer: Visualize Git Graph"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/vscode": "^1.68.0",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "@vscode/test-electron": "^2.1.3",
    "eslint": "^8.16.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.7.2"
  },
  "dependencies": {
    "d3": "^7.8.5",
    "element-resize-detector": "^1.2.4",
    "force-graph": "^1.42.9",
    "path": "^0.12.7",
    "simple-git": "^3.10.0"
  }
}
